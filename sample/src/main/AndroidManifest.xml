<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.buzzvil.buzzscreen.sample">
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Configuration for BuzzScreen -->

        <!--Optional: Opt-in mobvista mediation.-->
        <meta-data
            android:name="com.buzzvil.locker.mediation.mobvista.app_id"
            android:value="__app_id__" />

        <meta-data
            android:name="com.buzzvil.locker.mediation.mobvista.app_key"
            android:value="__app_key__" />
        
        <!--Optional: Opt-in mopub mediation.-->
        <meta-data
            android:name="com.buzzvil.locker.mediation.mopub.ad_unit_id"
            android:value="__ad_unit_id__" />

        <!--Optional: Opt-in admob mediation.-->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="__app_key__" />
    </application>
</manifest>
